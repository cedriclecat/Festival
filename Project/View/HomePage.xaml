<UserControl x:Class="Project.View.HomePage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="800">
    <Grid>
        <TextBlock x:Name="LblHome" HorizontalAlignment="Center" TextWrapping="Wrap" Text="Home" VerticalAlignment="Top" Height="50" Width="200" TextAlignment="Center" FontSize="20"/>
        <TextBlock x:Name="LblTime" HorizontalAlignment="Center" Margin="0,0,0,0" TextWrapping="Wrap" Text="Time:" VerticalAlignment="Center" Height="50" Width="200" TextAlignment="Center" FontSize="20"/>
    </Grid>
</UserControl>
